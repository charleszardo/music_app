<h4>Add Tag</h4>
<form action="<%= taggings_url %>" method="post">
<%= auth_token %>
<input type="hidden" name="tagging[taggable_type]" value="<%= taggable_type %>">
<input type="hidden" name="tagging[taggable_id]" value="<%= taggable_id %>">
<input type="text" name="tagging[tag]" id="tagging_tag">
<input type="submit" value="submit">
</form>
