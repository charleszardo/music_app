<h1><%= @album.title %></h1>
<h2>by <%= link_to @album.band_name, band_url(@album) %></h2>
<h3><%= @album.album_type %></h3>
<h3>Tracks</h3>
<% @album.tracks.each do |track| %>
  <p><%= link_to track_url(track) %></p>
<% end %>
<%= link_to "Add Track", new_track_url %>
<br>
<%= link_to "Edit Album", edit_album_url %>
<%= link_to "Delete Album", @album, method: :delete %>
<%= link_to @album.band_name, band_url(@album.band) %>
